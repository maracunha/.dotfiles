[user]
  name = Renan Marangon
  email = maracunha@gmail.com
[diff]
  indentHeuristic = true
  submodule = log
[color]
  diff = always
  interactive = always
  status = always
  branch = always
[alias]
  st = status
  ci = commit
  co = checkout
  br = branch
  fixup = commit --all --amend --no-edit
  open = !explorer `git config remote.origin.url`
  browse = !git open
  up = push upstream master
  sync = pull upstream master --rebase
  which = !git branch | grep -i
[pull]
  ff = only
[push]
  default = simple
[log]
  decorate = full
  abbrevCommit = true
[init]
  defaultBranch = main
